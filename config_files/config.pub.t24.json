{
    "main": {
        "data-folder": "/node/sm_data",
        "metrics-port": "10001",
        "metrics": true,
        "poet-server": [
            "https://poet-1.team24.co",
            "https://poet-2.team24.co",
            "https://poet-3.team24.co"
          ]
        },
    "poet": {
        "phase-shift": "288h",
        "cycle-gap": "24h",
        "grace-period": "2h"
    },
    "api": {
        "grpc-public-listener": "0.0.0.0:10192",
        "grpc-private-listener": "0.0.0.0:10193",
        "grpc-tls-listener": "0.0.0.0:10194",
        "grpc-tls-ca-cert": "/certs/ca.crt",
        "grpc-tls-cert": "/certs/server.crt",
        "grpc-tls-key": "/certs/server.key"
    },
    "p2p": {
        "listen": "/ip4/0.0.0.0/tcp/6001",
        "min-peers": 40,
        "low-peers": 50,
        "high-peers": 100
    },
    "logging": {
        "p2p": "error"
    },
    "smeshing-start": false
}